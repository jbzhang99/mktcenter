  <select id="getTaskList" parameterType="com.bizvane.mktcenterservice.models.vo.TaskVO" resultType="com.bizvane.mktcenterservice.models.vo.TaskVO">
    SELECT
    tmt.mkt_task_id,
    tmt.sys_company_id,
    tmt.sys_brand_id,
    tmt.task_code,
    tmt.task_type,
    tmt.task_name,
    tmt.start_time,
    tmt.end_time,
    tmt.task_info,
    tmt.points,
    tmt.task_list_img,
    tmt.task_detail_img,
    tmt.check_status,
    tmt.task_status,
    tmt.remark,
    tmt.create_user_id,
    tmt.create_user_name,
    tmt.create_date,
    tmt.modified_user_id,
    tmt.modified_user_name,
    tmt.modified_date,
    tmt.valid,
    tmti.mkt_task_invite_id,
    tmti.invite_num
    FROM
    `t_mkt_task` tmt
    INNER JOIN t_mkt_task_invite tmti ON tmt.mkt_task_id = tmti.mkt_task_id
    AND tmt.valid = 1
    AND tmti.valid = 1
    <!--任务主表ID -->
    <if test="mktTaskId != null and mktTaskId!= '' ">
      AND tmt.mkt_task_id = #{mktTaskId}
    </if>
    <if test="taskStatus != null and taskStatus!=0">
      AND tmt.activity_status = #{taskStatus}
    </if>
    <!--所属品牌id -->
    <if test="sysBrandId != null and sysBrandId!= ''">
      AND tmt.sys_brand_id = #{sysBrandId}
    </if>
  </select>
